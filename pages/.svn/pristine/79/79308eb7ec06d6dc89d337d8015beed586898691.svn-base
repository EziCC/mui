.mgWrap p span {
    flex: 1 !important;
    text-align: center;
}
.lampNum input {
	margin: 0;
}
.login_iconon{
	background: #2AC845 !important;
}
.login_icon{
	background: #cccccc;
	float: left;
    list-style: none;
    flex: 1;
    text-align: center;
    padding: 1px;
    margin: 1px;
    line-height: 3rem;
    font-size: 1rem;
    color: #ffffff;
}
#tip {
	background: rgba(0, 0, 0, 0.3);
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	display: none;
	z-index: 9999;
}

#tip .pack {
	width: 80%;
	min-height: 100px;
	margin: 0 auto;
	background: #FFF;
	margin-top: 30%;
	position: relative;
	overflow: hidden;
	padding-bottom: 60px;
	border-radius: 5px;
}
#tip .pack img{
	margin: 70px auto 20px;
	display: block;
}
#tip .pack h1 {
	font-size: 14px;
	text-align: center;
	color: #666666;
	line-height: 36px;
	margin-top: 20px;

}
#tip .pack a {
	display: block;
	width: 100%;
	position: absolute;
	bottom: 0px;
	font-size: 16px;
	color: #2AC845;
	height:40px;
	line-height: 40px;
	text-align: center;
	border-top: 1px solid #d6d6d6;
}
.tc {
    text-align: center!important;
}